<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.ieqmobile" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>IEQ - MOBILE</name>
    <description>
        A blank PhoneGap app.
    </description>
    <author email="saymonpscholz@gmail.com" href="http://phonegap.com">
        Saymon Possenti
    </author>
    
<preference name="phonegap-version" value="cli-8.0.0" />
<plugin name="cordova-plugin-statusbar" source="npm"/>
<plugin name="cordova-plugin-device" source="npm"/>
<plugin name="cordova-plugin-splashscreen" source="npm"/>
<plugin name="cordova-plugin-vibration" source="npm"/>
<plugin name="cordova-plugin-dialogs" source="npm"/>

<plugin name="cordova-plugin-camera" source="npm">
<variable name="CAMERA_USAGE_DESCRIPTION" value=" "/>
<variable name="PHOTOLIBRARY_USAGE_DESCRIPTION" value=" "/>
</plugin>
<plugin name="cordova-plugin-contacts" source="npm">
<variable name="CONTACTS_USAGE_DESCRIPTION" value=" "/>
</plugin>
<plugin name="cordova-plugin-file" source="npm">
<plugin name="cordova-plugin-file-transfer" source="npm"/>
    
    <content src="index.html" />
    <access origin="*" />
</widget>
